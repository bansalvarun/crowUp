<h1>New Branch</h1>
<%= form_for :field, url: fields_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
  <p><%= link_to 'All Fields', fields_path %></p>
<% end %>